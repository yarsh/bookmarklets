javascript:(function(){(function(){var page=document.querySelector('html');var players=document.querySelectorAll('video');var playbackSpeed=1;var min=0;var max=50;var l=players.length;var i=0;var player;var hash;for(i;i<l;i++){player=players[i];player.onwheel=function(evt){var speed=(evt.deltaY>0)?1:-1;var delta=((parseInt(playbackSpeed,10)+1)/10)*speed;playbackSpeed=parseFloat((playbackSpeed+delta).toFixed(1));if(playbackSpeed<min){playbackSpeed=min;}else{if(playbackSpeed>max){playbackSpeed=max;}}player.playbackRate=parseFloat(playbackSpeed);location.hash=location.hash.split('?speed=')[0]+'?speed='+playbackSpeed;console.log(playbackSpeed);return false;};}})();})();